{
	"manifest_version": 2,
	"name": "Yuu's Trello Suite",
	"short_name": "YuuTrelloSuite",
	"version": "1.0.0",
	"content_scripts": [{
		"matches": ["https://trello.com/*"],
		"run_at": "document_start",
		"js": ["css.js", "index.js"],
		"all_frames": true
	}],
	
	"permissions": [
		"storage"	
	],

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}