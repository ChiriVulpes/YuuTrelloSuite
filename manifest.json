{
	"manifest_version": 2,
	"name": "Trello List Toggle",
	"short_name": "TrelloListToggle",
	"version": "0.0.1",
	"content_scripts": [{
		"matches": ["https://trello.com/*"],
		"js": ["index.js"],
		"all_frames": true
	}],
	
	"permissions": [
		"storage"	
	],

    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}